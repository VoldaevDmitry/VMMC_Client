<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8"/>
    </startup>
	<system.serviceModel>
		<bindings>
			<basicHttpBinding>
				<binding name="BasicHttpBinding_IFileLoaderService" />
				<binding name="BasicHttpBinding_IFileGetterService"  maxReceivedMessageSize="2147483647" maxBufferSize="2147483647"  />
				<binding name="BasicHttpBinding_IFileDeleteService" />
			</basicHttpBinding>
		</bindings>
		<client>
			<endpoint address="http://server-db.vnp2.main:8080/FileLoaderService"
			  binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IFileLoaderService"
			  contract="FileLoaderServiceReference.IFileLoaderService" name="BasicHttpBinding_IFileLoaderService" />
			<endpoint address="http://server-db.vnp2.main:8080/FileGetterService"
			  binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IFileGetterService"
			  contract="FileGetterServiceReference.IFileGetterService" name="BasicHttpBinding_IFileGetterService" />
			<endpoint address="http://server-db.vnp2.main:8080/FileDeleteService"
			  binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IFileDeleteService"
			  contract="FileDeleteServiceReference.IFileDeleteService" name="BasicHttpBinding_IFileDeleteService" />
		</client>
	</system.serviceModel>
</configuration>
